(*------------------------------------prog starts here------------------------------------*)

{px,py}={1.6,-1.6};
n=3;
shapesgl=gl={};
Do[maxLOS=iter;
    plbl=ToString[lbl]<>" iter="<>ToString[iter]<>" "<>date;
    gl={};
    Do[type=i;
      {x,y}={px,py} {(i-1)-n Floor[(i-1)/n],Floor[(i-1)/n]};
      fig=getFigure[type,{x,y},0,1,{i}];
      shapecol=getColor[iter];
      flst=recursiveSubdiv[fig];
      AppendTo[gl,getGLst[flst]];
      ,{i,10,10}];
    p=Show[Graphics[{PointSize[.003],gl,shapesgl}],
        AspectRatio\[Rule]Automatic,
        PlotRange\[Rule]rng];
	Print[p];
    epsOut["p_"<>lbl<>"_iter"<>ToString[iter]<>".eps",p];
    ,{iter,0,niter}];
(*------------------------------------prog ends here------------------------------------*)




